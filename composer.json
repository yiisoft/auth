{
    "name": "yiisoft/auth",
    "type": "library",
    "description": "Yii auth",
    "keywords": [
        "auth",
        "middleware"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/auth/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/auth"
    },
    "require": {
        "php": "^7.4|^8.0",
        "psr/http-server-middleware": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "yiisoft/strings": "^1.0",
        "yiisoft/http": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "nyholm/psr7": "^1.3",
        "roave/infection-static-analysis-plugin": "^1.3",
        "vimeo/psalm": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Auth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Auth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phan": "phan --progress-bar -o analysis.txt",
        "test": [
            "phpunit --colors --no-interaction"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
